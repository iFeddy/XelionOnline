open [DungeonSuccess]
	questresult Suc.
	
	var RndNum 0.
        RndNum = @Random(1 7).
        
        if RndNum === "1"
        then open
            findhandle Player00 "player" 0.
			findhandle Player01 "player" 1.
			findhandle Player02 "player" 2.
			findhandle Player03 "player" 3.
			findhandle Player04 "player" 4.
	
			broadcast all "Everyone has received, [Crafting Piece] Red Cube!".
			itemcreate Player00 "CustomItemOrM98".
			itemcreate Player01 "CustomItemOrM98".
			itemcreate Player02 "CustomItemOrM98".
			itemcreate Player03 "CustomItemOrM98".
			itemcreate Player04 "CustomItemOrM98".
